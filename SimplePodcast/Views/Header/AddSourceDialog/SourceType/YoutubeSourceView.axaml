<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:viewmodel="clr-namespace:SimplePodcast.ViewModels"
             mc:Ignorable="d" d:DesignWidth="200"
             x:Class="SimplePodcast.Views.YoutubeSourceView"
             x:DataType="viewmodel:YoutubeSourceViewModel">
    <StackPanel Orientation="Vertical" Spacing="4">
        <TextBlock Text="Url"/>
        <TextBox Text="{CompiledBinding Url}" TextWrapping="NoWrap"></TextBox>
    </StackPanel>
</UserControl>
